{
  "name": "kilo-code-enhanced-vite-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "migrate": "node deploy-migrations.cjs",
    "migrate-status": "node deploy-migrations.cjs status",
    "analyze": "node scripts/code-analyzer.js",
    "docs:generate": "typedoc --out docs/generated src",
    "kilo-code:setup": "node scripts/setup-kilo-code.js",
    "kilo-code:analyze": "node scripts/kilo-code-tools.js analyze",
    "kilo-code:refactor": "node scripts/kilo-code-tools.js refactor"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.344.0",
    "pg": "^8.11.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.4",
    "react-toastify": "^11.0.5",
    "zod": "^3.23.8",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^3.0.0",
    "@vitest/coverage-v8": "^3.0.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdoc": "^4.0.3",
    "postcss": "^8.4.35",
    "supabase": "^2.48.3",
    "tailwindcss": "^3.4.1",
    "typedoc": "^0.26.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "react",
    "typescript",
    "vite",
    "kilo-code",
    "agentic-development"
  ]
}
